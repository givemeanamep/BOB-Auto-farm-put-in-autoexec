
local lo = workspace.Lobby
local function(bob) 
while true do
wait()
game.Players.LocalPlayer.Character.Humanoid.Health == 0
        repeat wait() 
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), workspace.Lobby.Teleport1, 0)
firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild("Head"), workspace.Lobby.Teleport1, 1)
    until game.Players.LocalPlayer.Character:FindFirstChild("entered") ~= nil 
wait(0.08)
    --put bob function here
 end
end
while true do
  wait()
  if game.Players.LocalPlayer.Character.isInArena.Value == false and game.Players.LocalPlayer.leaderstats.Glove.Value == "Replica" then
    fireclickdetector(lo.bob.ClickDetector)
    else if game.Players.LocalPlayer.leaderstats.Glove.Value == 'bob' and game.Players.LocalPlayer.Character.isInArena.Value == true then
      print('got bob')
      else if game.Players.LocalPlayer.leaderstats.Glove.Value ~= "bob"and game.Players.LocalPlayer.leaderstats.Glove.Value ~= "Replica" and game.Players.LocalPlayer.Character.isInArena.Value == false then print('lol nah none')
        fireclickdetector(lo.Replica.ClickDetector)
        else if game.Players.LocalPlayer.leaderstats.Glove.Value ~= "bob" and game.Players.LocalPlayer.leaderstats.Glove.Value == "Replica" and game.Players.LocalPlayer.Character.isInArena.Value == true then 
       
      end
end
  end
end
